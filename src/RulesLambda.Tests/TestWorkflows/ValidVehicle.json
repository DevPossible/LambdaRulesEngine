      {
        "RuleName": "VehicleModelMustBe3500",
        "SuccessEvent": "10",
        "ErrorMessage": "Vehicle model is not 3500.",
        "ErrorType": "Error",
        "RuleExpressionType": "LambdaExpression",
        "Expression": "Fn.GetVehicleData(vin,28)==\"3500\""
      }
    ]
  }
